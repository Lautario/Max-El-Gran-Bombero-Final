}

if move= -1  {
      image_xscale = -1;

}


if (place_meeting(x+hsp, y, objmuro)){
    while(!place_meeting(x+sign(hsp), y, objmuro)){
    x += sign (hsp);  
    }
    hsp = 0;
}
